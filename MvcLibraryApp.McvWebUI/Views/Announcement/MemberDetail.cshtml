@model MvcLibraryApp.McvWebUI.Models.Entity.Announcements
@{
    ViewBag.Title = "MemberDetail";
    Layout = "~/Views/Shared/_LayoutPanel.cshtml";
}


<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!-- The above 3 meta tags *must* come first in the head; any other head content must come *after* these tags -->

    <title>Duyuru Detayları</title>

    <!-- Google font -->
    <link href="https://fonts.googleapis.com/css?family=Josefin+Sans:400,700" rel="stylesheet">

    <!-- Custom stlylesheet -->
    <link type="text/css" rel="stylesheet" href="/Templates/Content/css/style.css" />


</head>

<body>

    <div id="notfound">
        <div class="notfound">
            <div>
                <h1 style="color:crimson">@DateTime.Parse(Html.DisplayFor(x => x.Date).ToString()).ToShortDateString()</h1>
            </div>
            <h3 style="color:aqua">@Html.DisplayFor(x => x.AnnouncementContent)</h3>
            <br />
            <a href="/Announcement/MemberPage/">Duyurular Sayfasına Dön</a>
        </div>
    </div>

</body>

</html>


