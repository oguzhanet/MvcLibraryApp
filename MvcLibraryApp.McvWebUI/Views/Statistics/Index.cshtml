    Layout = "~/Views/Shared/_Layout1.cshtml";
}

<h2>Index</h2>
<link href="~/Templates/web2/css/jqvmap.css" rel="stylesheet" />
<link href="~/Templates/web2/css/style.css" rel="stylesheet" />
<script src="~/Templates/web2/js/jquery.min.js"></script>
<script src="~/Templates/web2/js/jquery.vmap.js"></script>
<script src="~/Templates/web2/js/jquery.vmap.sampledata.js"></script>
<script src="~/Templates/web2/js/jquery.vmap.world.js"></script>
<script src="~/Templates/web2/js/Chart1.js"></script>
<script>
    $(document).ready(function (c) {
        $('.alert-close6').on('click', function (c) {
            $('.world-map').fadeOut('slow', function (c) {
                $('.world-map').remove();
            });
        });
    });
</script>
<script type="text/javascript">
    jQuery(document).ready(function () {
        jQuery('#vmap').vectorMap({
            map: 'world_en',
            backgroundColor: '#333333',
            color: '#ffffff',
            hoverOpacity: 0.7,
            selectedColor: '#666666',
            enableZoom: true,
            showTooltip: true,
            values: sample_data,
            scaleColors: ['#C8EEFF', '#006491'],
            normalizeFunction: 'polynomial'
        });
    });
</script>

